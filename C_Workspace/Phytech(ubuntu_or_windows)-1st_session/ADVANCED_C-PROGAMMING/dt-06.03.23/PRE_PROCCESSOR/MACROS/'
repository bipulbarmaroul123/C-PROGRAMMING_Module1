#include<stdio.h>
#define POSITIVE 10
#define NEGETIVE -20
int main()
{
#ifdef POSITIVE
	int a = 10, b = 20;
#endif
#ifdef POSITIVE
	int x = -10, x = -20;
#endif
	printf("positive number is: %d", POSITIVE);
	printf("negetive number is: %d", NEGETIVE);
	printf("the value of a and b is: %d %d", a, b);
	printf("the addition of two number is: %d", a+b);
	return 0;
}

